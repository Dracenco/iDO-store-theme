/* SHELF TEMPLATE*/

  "flex-layout.row#list-context-template": {
       "children": [
        "list-context.product-list#shelf-tutti-i-prodotti-template"
       ],
       "props": {
       "blockClass": "flex-row-shelf-home-page"
       }
   },

  "list-context.product-list#shelf-tutti-i-prodotti-template": {
    "blocks": ["product-summary.shelf#tutti-i-prodotti"],
    "children": ["slider-layout#shelf-tutti-i-prodotti"]
  },

  "slider-layout#shelf-tutti-i-prodotti": {
    "children": [
      "shelf#tutti-i-prodotti"
    ],
    "props": {
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 2,
        "phone": 1
      },
      "infinite": true,
      "fullWidth": true,
      "blockClass": "shelf-tutti-i-prodotti",
      "showNavigationArrows": "never",
      "autoplay": { "stopOnHover": true, "timeout": 4000},
      "slideTransition": { "speed": 1200, "delay": 0, "timing": "" }
    }
  },

  "shelf#tutti-i-prodotti": {
      "blocks": ["product-summary.shelf#tutti-i-prodotti"],
      "props": {
        "blockClass": "shelf-tutti-i-prodotti-radu",
        "category":2 ,
        "orderBy": "OrderByTopSaleDESC",
        "paginationDotsVisibility": "desktopOnly",
        "productList": {
          "maxItems": 4,
          "itemsPerPage": 4,
          "minItemsPerPage": 1,
          "scroll": "BY_PAGE",
          "arrows": true,
          "titleText": ""
        }
      }
    },
  "product-summary.shelf#tutti-i-prodotti": {
    "props": {
      "blockClass": "shelf-tutti-i-prodotti-radu",
      "isOneClickBuy": false,
      "showBadge": false,
      "badgeText": "ON",
      "displayBuyButton": "displayButtonHover",
      "showCollections": false,
      "showListPrice": false,
      "showLabels": false,
      "showInstallments": false,
      "showSavings": false,
      "showDiscount" : false
    },
        "children": [
          "product-summary-image",
          "product-summary-name",
          "product-summary-price",
          "add-to-list-btn"
        ]
  },

/* newsletter template*/

   "flex-layout.row#newslwtter-banner-template": {
       "children": [
        "info-card#newslwtter-banner-hp"
       ],
       "props": {
       "blockClass": "flex-row-all-static-pages"
       }
   },

  "info-card#newslwtter-banner-template": {
    "props": {
      "id": "info-card-fidelity-card",
      "isFullModeStyle": true,
      "textPosition": "left",
      "headline": "Iscriviti alla newsletter",
      "callToActionText": "ISCRIVITI",
      "callToActionUrl": "/iscriviti-alla-newsletter?__bindingAddress=www.ido.it/it",
      "blockClass": "newsletter-desktop",
      "textAlignment": "center"
    }
  }


}
