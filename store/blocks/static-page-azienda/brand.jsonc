   "info-card#brand-kitlook1": {
       "props": {
         "id": "brand-kitlook1",
         "isFullModeStyle": true,
         "textPosition": "left",
         "imageUrl": "/arquivos/585x812-landing-lookbook-girl-iDO-fw-2020.png",
         "headline": "COLLEZIONE<br>BAMBINA",
         "callToActionText": "SCOPRI",
         "callToActionUrl": "/sale/d",
