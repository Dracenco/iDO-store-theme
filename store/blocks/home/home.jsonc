    },

      "slider-layout#andrei-images": {
        "children": [
        "stack-layout#firstSlide",
        "stack-layout#secondSlide",
        "stack-layout#thirdSlide"
        ],
        "props": {
          "itemsPerPage": {
            "desktop": 1,
            "tablet": 1,
            "phone": 1
          },
          "infinite": true,
          "fullWidth": true,
          "showNavigationArrows": "never",
          "autoplay": { "stopOnHover": false, "timeout": 4000},
          "navigationStep": 1,
          "slideTransition": { "speed": 1200, "delay": 0, "timing": "" },
          "blockClass": "hatSlider"
        }
      },

     "stack-layout#firstSlide": {
           "props": {
            "blockClass": "hatSliderStack"
           },
            "children": [
            "info-card#hat1",
            "info-card#hat2"
            ]
          },
        "info-card#hat1": {
          "props": {
            "imageUrl": "/arquivos/hp_1250x570_slider_01.jpg",
            "isFullModeStyle": true,
            "callToActionText": "Shop Boy",
            "callToActionUrl": "/Tutti-i-Prodotti/Bambino",
            "textPosition": "center",
            "headline": "Eleganza e stile per i tuoi bimbi",
            "zIndexOffset": 1,
            "blockClass": "firstLayer"
          }
        },
         "info-card#hat2": {
           "props": {
             "imageUrl": "",
             "isFullModeStyle": true,
             "callToActionText": "Shop Girl",
             "callToActionUrl": "/Tutti-i-Prodotti/Bambina",
             "textPosition": "left",
             "zIndexOffset": 1,
             "headline": "Glam<br>collection",
              "blockClass": "secondLayer"
           }
     },


     "stack-layout#secondSlide": {
           "props": {
            "blockClass": "hatSliderStack"
           },
            "children": [
            "info-card#hat3",
            "info-card#hat4"
            ]
          },
        "info-card#hat3": {
          "props": {
            "imageUrl": "/arquivos/hp_1250x570_slider_02.jpg",
            "isFullModeStyle": true,
            "textPosition": "center",
            "callToActionMode": "",
            "headline": "Capi pratici e comodi",
            "zIndexOffset": 1,
            "blockClass": "firstLayer"
          }
        },
         "info-card#hat4": {
           "props": {
             "imageUrl": "",
             "isFullModeStyle": true,
             "callToActionText": "Scoprila subito!",
             "callToActionUrl": "/Tutti-i-Prodotti/Bambina",
             "textPosition": "left",
             "zIndexOffset": 1,
             "headline": "È arrivata la<br>linea Sport Time",
              "blockClass": "secondLayer"
           }
     },

     "stack-layout#thirdSlide": {
           "props": {
            "blockClass": "hatSliderStack"
           },
            "children": [
            "info-card#hat5",
            "info-card#hat6"
            ]
          },
        "info-card#hat5": {
          "props": {
            "imageUrl": "/arquivos/hp_1250x570_slider_03.jpg",
            "isFullModeStyle": true,
            "callToActionText": "Shop boy",
            "callToActionUrl": "/Tutti-i-Prodotti/Bambino",
            "textPosition": "center",
            "headline": "Giubbotti reversibili e animal free",
            "zIndexOffset": 1,
            "blockClass": "firstLayer"
          }
        },
         "info-card#hat6": {
           "props": {
             "imageUrl": "",
             "isFullModeStyle": true,
             "callToActionText": "Shop Girl",
             "callToActionUrl": "/Tutti-i-Prodotti/Bambina",
             "textPosition": "left",
             "zIndexOffset": 1,
             "headline": "Tutti al caldo<br>con iDO",
              "blockClass": "secondLayer"
           }
     },

/* shelf home page*/

   "flex-layout.row#list-context": {
       "children": [
        "list-context.product-list#shelf-tutti-i-prodotti"
       ],
       "props": {
       "blockClass": "flex-row-all-static-pages"
       }
   },

  "list-context.product-list#shelf-tutti-i-prodotti": {
    "blocks": ["product-summary.shelf#tutti-i-prodotti"],
    "children": ["slider-layout#shelf-tutti-i-prodotti"]
  },

  "slider-layout#shelf-tutti-i-prodotti": {
    "children": [
      "shelf#tutti-i-prodotti"
    ],
    "props": {
      "itemsPerPage": {
        "desktop": 3,
        "tablet": 2,
        "phone": 1
      },
      "infinite": true,
      "fullWidth": true,
      "blockClass": "shelf-tutti-i-prodotti",
      "showNavigationArrows": "never",
      "autoplay": { "stopOnHover": false, "timeout": 4000},
      "slideTransition": { "speed": 1200, "delay": 0, "timing": "" }
    }
  },

  "shelf#tutti-i-prodotti": {
      "blocks": ["product-summary.shelf#tutti-i-prodotti"],
      "props": {
        "blockClass": "shelf-tutti-i-prodotti",
        "category": 2,
        "orderBy": "OrderByTopSaleDESC",
        "paginationDotsVisibility": "desktopOnly",
        "productList": {
          "maxItems": 7,
          "itemsPerPage": 3,
          "minItemsPerPage": 1,
          "scroll": "BY_PAGE",
          "arrows": true,
          "titleText": ""
        }
      }
    },
  "product-summary.shelf#tutti-i-prodotti": {
    "props": {
      "isOneClickBuy": false,
      "showBadge": false,
      "badgeText": "ON",
      "displayBuyButton": "displayButtonHover",
      "showCollections": false,
      "showListPrice": false,
      "showLabels": false,
      "showInstallments": false,
      "showSavings": false,
      "showDiscount" : false
    },
        "children": [
          "product-summary-image",
          "product-summary-name",
          "product-summary-price",
          "add-to-list-btn"
        ]
  },

