{
  "store.custom#brand": {
      "blocks": [
        "responsive-layout.desktop#brand",
        "responsive-layout.mobile#brand"
      ]
    },
    "responsive-layout.desktop#brand": {
      "children": [
        "slider-layout#brand",
        "flex-layout.row#brand-title",
        "flex-layout.row#brand-info-cards",
        "flex-layout.row#brand-subtitle",
        "flex-layout.row#brand-kitlook",
        "flex-layout.row#newslwtter-banner-brand"
      ]
    },

    "responsive-layout.mobile#brand": {
      "children": [
        "slider-layout#brand-mobile",
        "flex-layout.row#brand-title-mobile",
        "flex-layout.row#brand-info-cards-mobile",
        "flex-layout.row#brand-subtitle-mobile",
        "flex-layout.row#brand-kitlook-mobile",
        "flex-layout.row#newslwtter-banner-brand-mobile"
       ]
    },

      "slider-layout#brand": {
        "children": [
        "flex-layout.row#brand-video-ido"
        ],
        "props": {
          "itemsPerPage": {
            "desktop": 1,
            "tablet": 1,
            "phone": 1
          },
          "infinite": false,
          "fullWidth": true,
          "pauseOnHover": true,
          "showNavigationArrows": "never",
          "showPaginationDots":"never",
          "autoplay": { "stopOnHover": true, "timeout": 4000},
          "navigationStep": 1,
          "slideTransition": { "speed": 1200, "delay": 0, "timing": "" },
          "blockClass": "hatSlider"
        }
      },

   "flex-layout.row#brand-video-ido": {
       "children": [
         "rich-text#link-brand"
       ],
       "props": {
         "preventVerticalStretch": true,
         "width": "100%",
         "blockClass": "flex-row-slider-home-page"
       }
    },

  "rich-text#link-brand": {
    "props": {
      "text": "​<div class='vtex-iframe-ido-video'> <iframe class='vtex-iframe-ido-video-inside' height='610' src='https://www.youtube.com/embed/f4WyNrrJzz4?playlist=f4WyNrrJzz4&loop=1&autoplay=1&mute=1' frameborder='0' style = 'width: 100%' allow='accelerometer;  autoplay; encrypted-media; gyroscope; picture-in-picture' allowfullscreen></iframe></div>",
      "blockClass": "vtex-iframe-ido-video"
    }
  },


  "flex-layout.row#brand-title": {
       "children": [
         "rich-text#brand-title"
       ],
       "props": {
       "blockClass": "flex-row-all-static-pages"
       }
   },

    "rich-text#brand-title": {
        "props": {
          "blockClass": "brand-title",
          "text":
          "Lorem ipsum dolor sit amet"
        }
    },



/* IMAGES BRAND*/

    "flex-layout.row#brand-info-cards": {
       "children": [
         "flex-layout.col#brand-image1",
         "flex-layout.col#brand-image2"
       ],
       "props": {
       "blockClass": "flex-row-all-static-pages-brand"
       }
    },

    "flex-layout.col#brand-image1": {
       "children": [
         "image#brand-image1",
         "rich-text#brand-title1"
       ],
       "props": {
       "blockClass": "flex-col-all-static-pages1-brand"
       }
    },

    "image#brand-image1": {
        "props": {
        "src": "/arquivos/brand_image1.png",
        "blockClass": "brand-image1"
        }
    },

    "rich-text#brand-title1": {
        "props": {
          "blockClass": "brand-title1",
          "text":
          "Ut enim ad minima veniam, quis nostrum exercitationem ullam corporis suscipit laboriosam, nisi ut aliquid ex ea commodi consequatur"
        }
    },

    "flex-layout.col#brand-image2": {
       "children": [
        "rich-text#brand-title2",
         "image#brand-image2"
       ],
       "props": {
       "blockClass": "flex-col-all-static-pages2-brand"
       }
    },

    "image#brand-image2": {
        "props": {
        "src": "/arquivos/brand_image2.png",
        "blockClass": "brand-image2"
        }
    },

    "rich-text#brand-title2": {
        "props": {
          "blockClass": "brand-title2",
          "text":
          "Sed ut perspiciatis unde omnis iste natus error sit voluptatem accusantium doloremque laudantium, totam rem aperiam"
        }
    },




/* brand subtitle*/

  "flex-layout.row#brand-subtitle": {
       "children": [
         "rich-text#brand-subtitle"
       ],
       "props": {
       "blockClass": "flex-row-all-static-pages"
       }
   },

    "rich-text#brand-subtitle": {
        "props": {
          "blockClass": "brand-subtitle",
          "text":
          "Scopri le collezioni iDO<br>Lorem ipsum dolor sit amet"
