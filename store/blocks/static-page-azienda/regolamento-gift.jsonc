{
  "store.custom#regolamento-gift": {
      "blocks": [
        "responsive-layout.desktop#regolamento-gift",
        "responsive-layout.mobile#regolamento-gift"
      ]
    },
    "responsive-layout.desktop#regolamento-gift": {
      "children": [
        "slider-layout#regolamento-gift",
        "flex-layout.row#regolamento-gift-title",
        "flex-layout.row#regolamento-gift-content",
