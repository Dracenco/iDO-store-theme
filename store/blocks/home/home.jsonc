        "flex-layout.row#outlet-home-page",
        "flex-layout.row#newslwtter-banner-hp"
      ]
    },

    "responsive-layout.mobile#home": {
      "children": [
        "slider-layout#andrei-images-mobile",
        "flex-layout.row#list-context-mobile",
        "flex-layout.row#title-Look-Book-mobile",
