    ]
  },

  "shelf#first-shelf-shop-by-look": {
    "blocks": ["product-summary.shelf"],
    "props": {
      "blockClass": "first-shelf",
      "category": 2,
      "orderBy": "OrderByTopSaleDESC",
      "paginationDotsVisibility": "desktopOnly",
      "productList": {
        "maxItems": 7,
        "itemsPerPage": 3.5,
        "minItemsPerPage": 1,
        "scroll": "BY_PAGE",
        "arrows": true,
        "titleText": ""
      }
    }
  },

  "info-card#shop-by-look": {
    "props": {
      "id": "info-card-shop-by-look",
