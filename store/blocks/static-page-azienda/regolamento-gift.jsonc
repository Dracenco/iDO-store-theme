      ]
    },

      "slider-layout#regolamento-gift": {
        "children": [
        "flex-layout.row#slider-regolamento-gift-info-card-1"
        ],
        "props": {
          "itemsPerPage": {
            "desktop": 1,
            "tablet": 1,
            "phone": 1
          },
          "infinite": false,
          "fullWidth": true,
          "pauseOnHover": true,
          "showNavigationArrows": "never",
          "showPaginationDots":"never",
          "blockClass": "hatSlider"
        }
      },



    "flex-layout.row#slider-regolamento-gift-info-card-1": {
       "children": [
         "stack-layout#slider-regolamento-gift-info-card-1"
       ],
       "props": {
       "blockClass": "flex-row-slider-home-page"
       }
    },

    "stack-layout#slider-regolamento-gift-info-card-1": {
           "props": {
            "blockClass": "flex-row-slider-home-page"
           },
            "children": [
            "info-card#slider-regolamento-gift-info-card-1"

            ]
          },

   "info-card#slider-regolamento-gift-info-card-1": {
       "props": {
         "id": "slider-home-page-info-card-1",
         "isFullModeStyle": true,
         "textPosition": "left",
         "imageUrl": "/arquivos/regolamento-template@4x.png",
         "headline": "Regolamento<br>Gift Card iDO",
         "subhead": "Regala lo stile iDO",
         "callToActionMode": false,
         "blockClass": "slider-home-page-info-card-1",
         "textAlignment": "left"
       }
     },

  "flex-layout.row#regolamento-gift-title": {
