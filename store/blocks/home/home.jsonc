{
  "store.home": {
    "blocks": [
      "responsive-layout.desktop#home",
      "responsive-layout.mobile#home"
    ]
  },
    "responsive-layout.desktop#home": {
      "children": [
        "slider-layout#andrei-images",
        "flex-layout.row#list-context",
        "flex-layout.row#title-Look-Book",
        "flex-layout.row#backToSchool",
