{
  "store.custom#shop-by-look": {
    "blocks": [
      "responsive-layout.desktop#shop-by-look",
      "responsive-layout.mobile#shop-by-look-mobile"
    ]
  },

  "responsive-layout.desktop#shop-by-look": {
    "children": [
      "shelf#first-shelf-shop-by-look",
      "info-card#shop-by-look",
      "shelf#second-shelf-shop-by-look",
      "shelf#third-shelf-shop-by-look"
    ]
